<div class="row">
    <div class="large-12 columns">
        <h2>Search Results</h2>
    </div>
</div>

<table>
    <tbody>
        <tr>
            <% var hist = Backbone.history.getFragment().substring(0, Backbone.history.getFragment().lastIndexOf("/")) %>
            <th></th>
            <th><a href="#/<%=hist %>/name">Name</a></th>
            <th><a href="#/<%=hist %>/kennel">Kennel</a></th>
            <th><a href="#/<%=hist %>/city">City</a></th>
            <th><a href="#/<%=hist %>/prov">Province</a></th>
            <th colspan="3"><a href="#/<%=hist %>/breed">Breeder Of:</a></th>
        </tr>
        <% for (var i=0 ; i < results.length; i++) { %>
        <tr data-id=<%=results[i].breederId %>>
                <td># <%=i + 1%></td>
                <td><a href="#/breeder/<%=results[i].breederId %>"><%=results[i].name %></a></td>
                <td><%=results[i].kennel %></td>
                <td><%=results[i].city %></td>
                <td><%=results[i].province %></td>
                <td>
                <% for (var j=0; j < results[i].breeds.length; j++) { %>
                <%=results[i].breeds[j].breedName %><br>
                <% } %>
                </td>
            </tr>
        <% } %>
    </tbody>
</table>
<div class="row">
    <div class="large-6 columns">
        <br>
        <a href="#<%=Backbone.history.getFragment() %>" class="small round button" id="searchButton">Previous 10 Entries</a>
        <br>
    </div>
    <div class="large-6 columns">
        <br>
        <a href="#" class="small round button" id="searchButton">Next 10 Entries</a>
        <br>
    </div>
</div>